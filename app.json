{
  "expo": {
    "name": "Mediterranean Kids",
    "slug": "mediterranean-kids",
    "scheme": "medkids",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],

    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9E6"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF9E6"
      }
    },

    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },

    "extra": {}
  }
}
